package com.hibernate.inh.tableperclass;


import jakarta.persistence.AttributeOverride;
import jakarta.persistence.AttributeOverrides;
import jakarta.persistence.Column;
import jakarta.persistence.Entity;

@Entity
public class RegularEmployee103 extends Employee103 {
	private float salary;
	private int bonus;

	public RegularEmployee103() {
		super();
	}

	public RegularEmployee103(int id, String name, float salary, int bonus) {
		super(id, name);
		this.salary = salary;
		this.bonus = bonus;
	}

	public RegularEmployee103(String name, float salary, int bonus) {
		super(name);
		this.salary = salary;
		this.bonus = bonus;
	}

	public float getSalary() {
		return salary;
	}

	public void setSalary(float salary) {
		this.salary = salary;
	}

	public int getBonus() {
		return bonus;
	}

	public void setBonus(int bonus) {
		this.bonus = bonus;
	}

	@Override
	public String toString() {
		return "\nRegularEmployee [salary=" + salary + ", bonus=" + bonus + ", toString()=" + super.toString() + "]";
	}

}
