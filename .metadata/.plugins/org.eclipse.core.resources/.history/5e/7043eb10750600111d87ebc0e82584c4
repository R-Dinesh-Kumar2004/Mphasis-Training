package demo.spring_core.java_config;

import org.springframework.context.ApplicationContext;
import org.springframework.context.annotation.AnnotationConfigApplicationContext;

import demo.spring_core.model.Certificate;
import demo.spring_core.model.Student;

public class Ex02SpringJavaConfigDemo {
    public static void main( String[] args ){
        ApplicationContext context = new AnnotationConfigApplicationContext(JavaConfiguration.class);
        
        Certificate certificate = (Certificate) context.getBean("getCertificate");
        System.out.println(certificate);
        
        Student student = (Student) context.getBean("getStudent");
        System.out.println(student);
    }
}