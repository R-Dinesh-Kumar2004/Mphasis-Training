	<bean name="cert1" class="demo.spring_core.model.Certificate">
		<constructor-arg name="id">
		<value>102</value>
		</constructor-arg>
		<constructor-arg name="title" value="OCJA"/>
		<constructor-arg name="fees" type="double" value="12000"></constructor-arg>
	</bean>

	<bean name="student" class="demo.spring_core.model.Student">
		<property name="id" value="101" />
		<property name="name" value="Mohan" />
		<property name="subjects">
			<array>
				<value>Math</value>
				<value>Science</value>
				<value>Biology</value>
				<value>Physics</value>
			</array>
		</property>
		<property name="activities">
			<list>
				<value>Cricket</value>
				<value>Gym</value>
				<value>Learning</value>
			</list>
		</property>
		<property name="scores">
			<map>
				<entry key="SpellBee" value="98" />
				<entry key="Math Olympiad" value="95" />
				<entry key="Annual Exam" value="85" />
